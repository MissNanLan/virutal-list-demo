{
  "git": {
    "commitMessage": "chore: release111 v${version}",
    "commit": true,
    "tag": true,
    "push": true,
    "requireCleanWorkingDir": false
  },
  "hooks": {
    "before:init": ["npx --no-install lint-staged --quiet"]
  },
  "plugins": {
    "@release-it/conventional-changelog": {
      "preset": "angular",
      "infile": "CHANGELOG.md"
    }
  },
  "github": {
    "release": true
  },
  "npm": {
    "publish": false
  }
}
